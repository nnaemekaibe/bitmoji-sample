<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/lato_regular"

        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/lato_regular"
        />
    <font
        android:fontStyle="italic"
        android:fontWeight="400"
        android:font="@font/lato_italic"

        app:fontStyle="italic"
        app:fontWeight="400"
        app:font="@font/lato_italic"
        />
    <font
        android:fontStyle="normal"
        android:fontWeight="600"
        android:font="@font/lato_bold"


        app:fontStyle="normal"
        app:fontWeight="600"
        app:font="@font/lato_bold"
        />
    <font
        android:fontStyle="italic"
        android:fontWeight="600"
        android:font="@font/lato_bold_italic"

        app:fontStyle="italic"
        app:fontWeight="600"
        app:font="@font/lato_bold_italic"
        />

</font-family>